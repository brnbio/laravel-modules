{
    "name": "brnbio/laravel-modules",
    "description": "Modules system for laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Frank Heider",
            "email": "frank@brnb.io"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Brnbio\\Modules\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brnbio\\Modules\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Brnbio\\Modules\\ModulesServiceProvider"
            ]
        }
    }
}
